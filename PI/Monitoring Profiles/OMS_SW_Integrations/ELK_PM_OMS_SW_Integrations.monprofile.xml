			<profile:simpleContent profile:name="OrderNo" profile:dataType="string" profile:targetNamespace="">
				<!-- MF_OMS_VERTEX_Tax_Calculate -->
				<profile:valueQuery profile:queryText="$Body/*[local-name()='Order']/@*[local-name()='OrderNo']" /> 
